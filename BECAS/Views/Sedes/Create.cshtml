@model BECASLC.Sede
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Create";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Create</h1>

<h4>Sede</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="IdSocio" class="control-label"></label>
                <select asp-for="IdSocio" class="form-control" asp-items="ViewBag.IdSocio"></select>
            </div>
            <div class="form-group">
                <label asp-for="IdZona" class="control-label"></label>
                <select asp-for="IdZona" class="form-control" asp-items="ViewBag.IdZona"></select>
            </div>
            <div class="form-group">
                <label asp-for="IdPrograma" class="control-label"></label>
                <select asp-for="IdPrograma" class="form-control" asp-items="ViewBag.IdPrograma"></select>
            </div>
            <div class="form-group">
                <label asp-for="IdCatSede" class="control-label"></label>
                <select asp-for="IdCatSede" class="form-control" asp-items="ViewBag.IdCatSede"></select>
            </div>
            <div class="form-group">
                <label asp-for="Activo" class="control-label"></label>
                @*<input type="checkbox" asp-for="Activo" class="form-control" />*@
                @Html.CheckBox("IsChecked",new { @class = "form-control", @name = "Activo" })
                <span asp-validation-for="Activo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

